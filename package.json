{
  "name": "bigcorp-lotteries-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build-client": "cd client && npm run build ",
    "build:babel": "babel server.js -d build-babel --extensions '.ts'",
    "build:tsc": "tsc server.js --outDir build-tsc",
    "dev": "NODE_ENV=development nodemon --exec ts-node -r dotenv/config server.js",
    "prod": "npm run build-client && NODE_ENV=production ts-node -r dotenv/config server.js",
    "check-types": "tsc --noEmit",
    "finalize-lottery": "node -r dotenv/config scripts/finalizeLottery.mjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "random": "^4.1.0",
    "redis": "^4.6.7",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/node": "^20.3.1",
    "dotenv": "^16.1.4",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
